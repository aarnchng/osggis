namespace osgGIS
{
    class OSGGIS_EXPORT RemoveHolesFilter : public FeatureFilter
    {
    public:
        RemoveHolesFilter();
        
        virtual ~RemoveHolesFilter();
        
    public:
        FeatureList process( Feature* input, FilterEnv* env );
    };
}

#endif // _OSGGIS_REMOVE_HOLES_FILTER_H_